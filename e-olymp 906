/*Product of digits
Three digital number is given. Find the product of its digits.

Input
One three digital positive integer n.

Output
Print the product of digits in n.*/



#include <iostream>
#include <cmath>
using namespace std;

int main()
{
    int n,a,b,c;
        cin >> n;
        a = n / 100;
        b = n / 10 % 10;
        c = n % 10;
        cout << a*b*c;
  

    return 0;
}
